# DISCORD_TOKEN=your_discord_bot_token_here

# Web Dashboard Configuration
# Set to "true" to enable the web dashboard
DASHBOARD_ENABLED=false
# Port for the web dashboard (default: 8080)
DASHBOARD_PORT=8080